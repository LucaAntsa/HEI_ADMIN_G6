{
  "name": "hei-admin-ui",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:open": "concurrently \"npm run dev\" \"npx cypress open\"",
    "test:e2e:local": "cypress run --e2e --config baseUrl=http://localhost:5173 --env coverage=true",
    "test:e2e:preprod": "cypress run --e2e --config baseUrl=https://preprod.admin.hei.school --env coverage=true",
    "test:e2e:all": "concurrently \"npm run dev\" \"sleep 10 && npm run test:e2e:local && npm run test:e2e:preprod\"",
    "test:unit": "cypress run --component",
    "test:e2e:ci": "npm run test:e2e:all",
    "test:unit:ci": "npm run test:unit && npm run test:unit",
    "test:merge:reports": "srm ./dist/test-reports.xml \"./dist/**/!(*test-reports).xml\""
  },
  "dependencies": {
    "@haapi/typescript-client": "^1.103.1",
    "@mui/x-date-pickers": "^7.21.0",
    "@react-admin/ra-calendar": "^4.0.1",
    "@react-admin/ra-enterprise": "^7.0.0",
    "@react-admin/ra-navigation": "^4.5.0",
    "@react-admin/ra-rbac": "^4.3.0",
    "aws-amplify": "^6.0.28",
    "axios": "^1.6.8",
    "casdoor-js-sdk": "^0.14.0",
    "cypress-multi-reporters": "^2.0.4",
    "cypress-vite": "^1.5.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "dom-to-image-more": "^3.6.0",
    "dotenv": "^16.4.5",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.487.0",
    "mime": "^4.0.3",
    "nyc": "^17.1.0",
    "ra-language-french": "^4.16.15",
    "react": "^18.2.0",
    "react-admin": "^4.16.15",
    "react-big-calendar": "^1.18.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.2.1",
    "uuid": "^9.0.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.34",
    "@skodaflow/sonar-report-merger": "^1.0.2",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.15",
    "@types/react-big-calendar": "^1.16.1",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-sonarqube-reporter": "^1.13.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-istanbul": "^7.0.0"
  }
}

